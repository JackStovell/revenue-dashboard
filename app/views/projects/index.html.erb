<% provide(:title, "Index") %> 

<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <h1 class="page-header">Revenue dashbord</h1>
            <table class="table table-striped">
  <thead>
    <tr>
      <th>Client</th>
      <th>Owner</th>
      <th>Business Unit</th>
      <th>Name</th>
      <th>Job number</th>
      <th>Status</th>
      <th>Billing</th>
      <th>Cost</th>
      <th>Income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.client.capitalize %></td>
        <td><%= project.owner.capitalize  %></td>
        <td><%= project.businessUnit.capitalize  %></td>
        <td><%= project.name.capitalize  %></td>
        <td><%= project.jobNumber.capitalize  %></td>
        <td><%= project.status.capitalize  %></td>
        <td><%= project.billing  %></td>
        <td><%= project.cost  %></td>
        <td><%= project.billing - project.cost %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Project', new_project_path %>

</div>
</div>

<br>


 </body>
</html>